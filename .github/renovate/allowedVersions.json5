{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  packageRules: [
    {
      matchManagers: ["helm", "helm-values", "docker"],
      matchPackageNames: ["ghcr.io/home-operations/radarr"],
      allowedVersions: "<6",
    },
    {
      matchManagers: ["helm", "helm-values", "docker"],
      matchPackageNames: ["ghcr.io/home-operations/lidarr"],
      allowedVersions: "<3",
    },
    {
      "matchManagers": ["helmv3"],
      "matchDatasources": ["helm"],
      "matchPackageNames": ["app-template"],
      "enabled": false
    }
  ],
}
